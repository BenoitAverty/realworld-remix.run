{
  "compilerOptions": {
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ES2019"
    ],
    "esModuleInterop": true,
    "jsx": "react",
    "moduleResolution": "node",
    "target": "es2019",
    "strict": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "./app/*"
      ]
    }
  },
  "include": ["remix.env.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": [
    "../src/http/remix/node_modules"
  ]
}
